{
  "expo": {
    "name": "SecureIN Admin",
    "slug": "securein-admin",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/ADMIN.png",
    "scheme": "secureinadmin",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera",
          "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone",
          "recordAudioAndroid": true
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {}
    },
    "android": {
      "package": "com.veronica_ramala.secureinadmin"
    }
  }
}
